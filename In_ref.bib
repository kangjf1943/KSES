	file = {Dangulla et al. 2019.pdf:C\:\\Users\\kangj\\Documents\\OneDrive\\Zotero\\storage\\LQVESMRK\\Dangulla et al. 2019.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\kangj\\Documents\\OneDrive\\Zotero\\storage\\EWU3MR9Y\\S1618866718306460.html:text/html}
