@article{turnbull_understanding_2016,
	title = {Understanding the value of plant diversity for ecosystem functioning through niche theory},
	volume = {283},
	url = {https://royalsocietypublishing.org/doi/full/10.1098/rspb.2016.0536},
	doi = {10.1098/rspb.2016.0536},
	abstract = {Biodiversity experiments have generated robust empirical results supporting the hypothesis that ecosystems function better when they contain more species. Given that ecosystems provide services that are valued by humans, this inevitably suggests that the loss of species from natural ecosystems could diminish their value. This raises two important questions. First, will experimental results translate into the real world, where species are being lost at an alarming rate? And second, what are the benefits and pitfalls of such valuation exercises? We argue that the empirical results obtained in experiments are entirely consistent with well-established theories of species coexistence. We then examine the current body of work through the lens of niche theory and highlight where closer links with theory could open up opportunities for future research. We argue that niche theory predicts that diversity–functioning relationships are likely to be stronger (and require more species) in the field than in simplified experimental settings. However, we caution that while many of the biological processes that promote coexistence can also generate diversity–function relationships, there is no simple mapping between the two. This implies that valuation exercises need to proceed with care.},
	number = {1844},
	urldate = {2020-09-08},
	journal = {Proceedings of the Royal Society B: Biological Sciences},
	author = {Turnbull, Lindsay A. and Isbell, Forest and Purves, Drew W. and Loreau, Michel and Hector, Andy},
	month = dec,
	year = {2016},
	note = {Publisher: Royal Society},
	pages = {20160536},
	annote = {生态位理论和BES
有点困难……
提到了ES之间的关系较少被研究。},
	file = {Full Text PDF:C\:\\Users\\kangj\\Documents\\OneDrive\\Zotero\\storage\\IVDC6AG6\\Turnbull et al. - 2016 - Understanding the value of plant diversity for eco.pdf:application/pdf;Snapshot:C\:\\Users\\kangj\\Documents\\OneDrive\\Zotero\\storage\\6G8K76EK\\rspb.2016.html:text/html}
}

@article{__2002-1,
	title = {生物多样性与生态系统功能: 进展与争论},
	volume = {10},
	shorttitle = {生物多样性与生态系统功能},
	number = {1},
	journal = {生物多样性},
	author = {{张全国} and {张大勇}},
	year = {2002},
	pages = {49--60},
	annote = {BES理论分类及经典实验
机制列表：

选择概率效应和抽样效应：物种多的系统更可能包含高功能物种，导致功能较高。\# 但是这样的实验设计中如何考虑丰度？比如：A样地包含5个高产物种但每个物种只有1个个体，B样地包含3个低产物种，但每个物种有3个个体，哪个生产力更高？\# 此外，抽样效应还可以结合生物学机制详细讨论：一个高产的物种被抽到/或者几个互补的高产物种被同时抽到的概率有多大？一个能够促进其他物种生长的物种被抽到的概率有多大？\# 不存在超产现象：如果有豆科植物呢？以及超产现象如何定义，毕竟产量和施肥营养状况相关；当然还有一个问题是，施肥在增加生物量的同时通常也降低了物种丰富度。\# 该机制可能证明BES机制的不确定性假说：BES是由物种性状决定的。
统计均衡效应：来自经济学的投资组合和风险的关系，生物量稳定性随着物种丰富度增加而升高；展示了一个数学证明。
生态位互补效应：生态位各异 - 生物多样性高的生态系统能更充分地利用环境资源 - 增进生态系统功能；且这种关系和物种间差异正相关；据资源竞争模型进行理论解释。\# 乔木和灌木草本之间是否存在互补效应？
种间正相互作用：例如豆科对其他植物的促进作用 - 超产现象。\# 乔木和某些耐阴草本是否存在相互作用？
保险假说：物种丰富度提高生产力和稳定性 - 原因在于时间生态位分化。
冗余机制：确定冗余种 - 功能群；在评估长期价值的时候，冗余种也很重要，此时可以被归入铆钉机制的时间维度上的保险假说。导致冗余的过程：生态位相似；有些物种完全依靠迁入维持。
 
争议：
是物种多样性，还是物种组成 - 性状和功能群 - 决定EF？
物种多样性的关键水平 - 物种丰富度/均匀度/多样性指数；种间差异；功能群多样性；功能群内物种数；还是食物网复杂性；也可能不同水平的多样性同时生效？
种群稳定性和功能稳定性。
 
代表性实验：
生态箱实验：主要结论：BEF正相关；批评：非随机的物种选取，高多样性的箱子中个体高的植物更多。
Tilman的Cedar Creek草地实验：对干旱的抵抗力和恢复力结论：richness高的抵抗力和恢复力也高；批评：实验草地上的richness由施肥造成，施肥本身导致richness降低，而且施肥改变植物生理导致抗性下降。改进实验：从物种库中随机选取物种构建中宇宙后，主要结论是BEF正相关，但仍被批评可能是选择概率效应。
Naeem的微宇宙实验：结论：功能群数相同下，功能群内物种数越高，系统可靠度越高。批评：功能群内物种数高的系统物种组成越相近。
欧洲草地实验：结论：群落生产力随着物种数减少而减少 - 可能原因是抽样效应、生态位互补和种间正相互作用。批评：去除物种的做法没有模拟自然灭绝过程；未测量单作产量，可能高估了超产现象；超产主要来自豆科，并非物种多样性的效果等。},
	file = {张全国 and 张大勇 - 2002 - 生物多样性与生态系统功能 进展与争论.pdf:C\:\\Users\\kangj\\Documents\\OneDrive\\Zotero\\storage\\MTXCBM3L\\张全国 and 张大勇 - 2002 - 生物多样性与生态系统功能 进展与争论.pdf:application/pdf;Snapshot:C\:\\Users\\kangj\\Documents\\OneDrive\\Zotero\\storage\\D5BMVVET\\49.html:text/html}
}

