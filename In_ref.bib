@article{brunbjerg_can_2018,
	title = {Can patterns of urban biodiversity be predicted using simple measures of green infrastructure?},
	volume = {32},
	issn = {1618-8667},
	url = {https://www.sciencedirect.com/science/article/pii/S1618866717301607},
	doi = {10.1016/j.ufug.2018.03.015},
	abstract = {Urban species and habitats provide important ecosystem services such as summertime cooling, recreation, and pollination at a variety of scales. Many studies have assessed how biodiversity responds to urbanization, but little work has been done to try and create recommendations that can be easily applied to urban planning, design and management practice. Urban planning often operates at broad spatial scales, typically using relatively simplistic targets for land-cover mix to influence biodiversity and ecosystem service provision. Would more complicated, but still easily created, prescriptions for urban vegetation be beneficial? Here we assess the importance of vegetation measures (percentage vegetation cover, tree canopy cover and variation in canopy height) across four taxonomic groups (bats, bees, hoverflies and birds) at multiple spatial scales (100, 250, 500, 1000 m) within a major urban area (Birmingham, the United Kingdom). We found that small-scale (100–250-m radius) measures of vegetation were important predictors for hoverflies and bees, and that bats were sensitive to vegetation at a medium spatial-scale (250–500 m). In contrast, birds responded to vegetation characteristics at both small (100 m) and large (1000 m) scales. Vegetation cover, tree cover and variation in canopy height were expected to decrease with built surface cover; however, only vegetation cover showed this expected trend. The results indicate the importance of relatively small patches of vegetationfor supporting urban biodiversity, and show that relatively simple measures of vegetation characteristics can be useful predictors of species richness (or activity density, in the case of bats). They also highlight the danger of relying upon percentage built surface cover as an indicator of urban biodiversity potential.},
	language = {en},
	urldate = {2021-03-25},
	journal = {Urban Forestry \& Urban Greening},
	author = {Brunbjerg, Ane Kirstine and Hale, James D. and Bates, Adam J. and Fowler, Robert E. and Rosenfeld, Emma J. and Sadler, Jonathan P.},
	month = may,
	year = {2018},
	keywords = {Bat, Bee, Bird, Hoverfly, Tree canopy height, Vegetation cover},
	pages = {143--153},
	annote = {BES？},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\kangj\\Documents\\OneDrive\\Zotero\\storage\\RYVEJLRH\\Brunbjerg et al. - 2018 - Can patterns of urban biodiversity be predicted us.pdf:application/pdf}
