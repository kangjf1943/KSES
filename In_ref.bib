@phdthesis{gyllin_biological_2004,
	title = {Biological {Diversity} in {Urban} {Environments}},
	abstract = {The thesis approaches the concept of urban biodiversity from different angles in an attempt to explain its significance. In a study from the constructed Toftanäs wetland park, methods of affecting local biodiversity are demonstrated as integrated with other functions, such as water quality and stormwater detention. Vegetation analyses are provided to show the rapid and sometimes unexpected change in species composition. Both spontaneous and introduced species were followed in a five-year project. Theoretical aspects of urban biodiversity are studied in-depth and the different views on biodiversity in general and urban biodiversity in particular are scrutinised. It is concluded that a holistic view on urban biodiversity probably reflects the true conditions best. In a method study, a few different kinds of biodiversity mapping were tested. It was concluded that the biotope-mapping model tested was easy and rapid, but incomplete. Patch mapping was more time-consuming, but possibly more informative. Patch shape was compared to species diversity, but there was no correlation. It was concluded that both kinds of mapping need to be calibrated with biological data, and that the human function of urban environments should be an integrated part of urban biodiversity studies. A method of assessing the perception of biodiversity was tested using a semantic test onsite. An index for biodiversity experience was created and later tested also in a photo-based study. Both studies comprised laymen as well as experts. It was concluded that there were differences between photo-based and on-site ratings, but the biggest difference was detected with the on-site experts. As a general conclusion of the thesis it can be stated that urban biodiversity is an integral part of the urban environment, and that it is impossible to regard the city without its biological component, as well as it is impossible to regard the biological component without its human connection.},
	language = {en},
	author = {Gyllin, Mats},
	year = {2004},
	file = {Gyllin-.pdf:C\:\\Users\\kangj\\Documents\\OneDrive\\Zotero\\storage\\DYWSMI57\\Gyllin-.pdf:application/pdf}
}

@article{troy_predicting_2007,
	title = {Predicting {Opportunities} for {Greening} and {Patterns} of {Vegetation} on {Private} {Urban} {Lands}},
	volume = {40},
	issn = {0364-152X, 1432-1009},
	url = {http://link.springer.com/10.1007/s00267-006-0112-2},
	doi = {10.1007/s00267-006-0112-2},
	language = {en},
	number = {3},
	urldate = {2019-09-12},
	journal = {Environmental Management},
	author = {Troy, Austin R. and Grove, J. Morgan and O’Neil-Dunne, Jarlath P. M. and Pickett, Steward T. A. and Cadenasso, Mary L.},
	month = sep,
	year = {2007},
	note = {Number: 3},
	keywords = {theory},
	pages = {394--412},
	annote = {Research Note
记忆标签：private; Baltimore；社会学
Baltimore, Maryland
Explore population / social / life style {\textasciitilde} possible stewardship \& realized stewardship.
Possible stewardship - plantable area to total area
Realized stewardship - area with plants to total area
theories for stewardship:
for possible stewardship - push and pull processes along time scale; housing filtering model that with aging and depreciating, richer people move out to larger house in suburb thus higher possible stewardship.
for realized stewardship - population drives development thus greening; social stratification that the richer invest more in greening or choose the environment; lifestyle behavior, aka "ecology prestige", greening are driven by personal id of community with certain life style.
Result
Best models:
PRIZM 15 - social stratification {\textasciitilde} possible stewardship;
PRIZM 62 - life style {\textasciitilde} realized stewardship.
housing age, pop density, vacancy etc. {\textasciitilde} both stewardship.
 
\# What is the relationship among location, wealth and legacy? If the richer live in suburban, while the oldest buildings locate in city center?
 
 
推送信息：
住宅区绿化：格局和机遇
城市居民的社会经济水平和生活方式如何影响城市绿化？一个美国马里兰州巴尔的摩市案例研究。
 
城市私有地对城市绿化的重要性非常重要。尽管我们对城市绿化的一般影响因素作了很多研究，但对城市内部居民区绿化的异质性却知之甚少。Troy等以巴尔的摩市居民区为例，对不同空间尺度上的绿化率及其影响因素进行分析，验证了人口密度、社会分层、生活方式的影响。本文发表在《Environmental Management》上。
居民区团组划分数据：来自咨询公司Claritas。在该数据中城市居民区首先按照城市化格局在空间上分成大类，包括城市、郊区和二级城市（second city）3个大类；接着这些大类按照社会分层被分成亚类；最后依据生活方式有关的变量将其进一步分成小类。
城市居民属性数据：如居民人口密度、房屋年代、犯罪率、收入水平、教育水平、房屋面积、结婚率等。
居民区绿化率数据：包括绿化潜力和实际绿化率，前者是可种植面积所占比例，后者则是绿化面积所占比例。
居民绿化支出数据。
 
团组类别和绿化率：不同空间尺度上的居民区团组类别的绿化率差异ANOVA分析表明，以社会分层为依据进行划分的团组亚类对绿化潜力的解释力最佳，而以生活方式为划分依据的团组小类能更好地预测实际绿化率和居民绿化支出。
居民属性和绿化率：对应上一步ANOVA分析结果，采用多元回归探究了社会分层类属性变量对绿化潜力的影响，以及分别探究了生活方式类属性变量对实际绿化率和居民绿化支出的影响。
 
前者分析表明：人口密度、房屋空置率、犯罪率等对绿化潜力有负效应，收入水平则对其有正效应，房屋年代的效应则呈凸曲线状。
 
后者分析表明：对于实际绿化率，同样地人口密度和房屋空置率有负效应，此外生活方式类中的联排房屋占比也有负效应，而教育水平、非裔美国人占比、和房屋面积、结婚率等则有正效应；而对于绿化支出，人口密度、非裔美国人占比则产生负效应，而意料之内地，收入水平和房屋面积等则有正效应。},
	file = {Troy et al.-2007.pdf:C\:\\Users\\kangj\\Documents\\OneDrive\\Zotero\\storage\\AYECIF2H\\Troy et al.-2007.pdf:application/pdf}
}

@article{shams_town_2019,
