@article{liu_carbon_2012,
	title = {Carbon storage and sequestration by urban forests in {Shenyang}, {China}},
	volume = {11},
	issn = {1618-8667},
	url = {http://www.sciencedirect.com/science/article/pii/S1618866711000227},
	doi = {10.1016/j.ufug.2011.03.002},
	abstract = {Urban forests can play an important role in mitigating the impacts of climate change by reducing atmospheric carbon dioxide (CO2). Quantification of carbon (C) storage and sequestration by urban forests is critical for the assessment of the actual and potential role of urban forests in reducing atmospheric CO2. This paper provides a case study of the quantification of C storage and sequestration by urban forests in Shenyang, a heavily industrialized city in northeastern China. The C storage and sequestration were estimated by biomass equations, using field survey data and urban forests data derived from high-resolution QuickBird images. The benefits of C storage and sequestration were estimated by monetary values, as well as the role of urban forests on offsetting C emissions from fossil fuel combustion. The results showed that the urban forests in areas within the third-ring road of Shenyang stored 337,000t C (RMB92.02 million, or \$ 13.88 million), with a C sequestration rate of 29,000t/yr (RMB7.88 million, or \$ 1.19 million). The C stored by urban forests equaled to 3.02\% of the annual C emissions from fossil fuel combustion, and C sequestration could offset 0.26\% of the annual C emissions in Shenyang. In addition, our results indicated that the C storage and sequestration rate varied among urban forest types with different species composition and age structure. These results can be used to help assess the actual and potential role of urban forests in reducing atmospheric CO2 in Shenyang. In addition, they provide insights for decision-makers and the public to better understand the role of urban forests, and make better management plans for urban forests.},
	language = {en},
	number = {2},
	urldate = {2020-09-17},
	journal = {Urban Forestry \& Urban Greening},
	author = {Liu, Changfu and Li, Xiaoma},
	month = jan,
	year = {2012},
	keywords = {Ecosystem service, Urban ecosystem, Carbon density, Carbon emission, Carbon sequestration rate, Globe warming},
	pages = {121--128},
	file = {ScienceDirect Snapshot:C\:\\Users\\kangj\\Documents\\OneDrive\\Zotero\\storage\\5J6P3F6V\\S1618866711000227.html:text/html;ScienceDirect Full Text PDF:C\:\\Users\\kangj\\Documents\\OneDrive\\Zotero\\storage\\XBMUQ7MI\\Liu and Li - 2012 - Carbon storage and sequestration by urban forests .pdf:application/pdf}
}

@article{tang_carbon_2016,
	title = {Carbon {Storage} and {Sequestration} of {Urban} {Street} {Trees} in {Beijing}, {China}},
	volume = {4},
	issn = {2296-701X},
	url = {https://www.frontiersin.org/articles/10.3389/fevo.2016.00053/full},
	doi = {10.3389/fevo.2016.00053},
	abstract = {Anthropogenic carbon dioxide (CO2) emissions from cities have significantly increased over the past several decades along with rapid urbanization. To what extent anthropogenic CO2 emissions generated from cities can be offset through conserving or increasing carbon (C) stored within urban areas themselves is a significant scientific question. The role of urban street trees in offsetting anthropogenic CO2 emissions still remains uncertain. Here, using data from field surveys, tree growth measurements and governmental statistical yearbooks, we estimated the C storage and C sequestration capacity of street trees in Beijing. Results showed that the C density and C sequestration rate in Beijing’s urban street trees were about 1/3 {\textasciitilde}1/2 of the corresponding magnitudes of non-urban forests in China. However, the total C sequestration of street trees in Beijing’s urban districts was 3.1±1.8 Gg C yr-1 (1 Gg = 109 g) in 2014, equivalent to only about 0.2\% of its annual CO2-equivalent emissions from total energy consumption, indicating a rather limited role in offsetting overall anthropogenic CO2 emissions.},
	language = {English},
	urldate = {2020-09-17},
	journal = {Frontiers in Ecology and Evolution},
	author = {Tang, Yujia and Chen, Anping and Zhao, Shuqing},
	year = {2016},
	note = {Publisher: Frontiers},
	keywords = {Urbanization, Urban vegetation, Urban sustainability, Carbon Cycle, Tree benefits},
	file = {Full Text PDF:C\:\\Users\\kangj\\Documents\\OneDrive\\Zotero\\storage\\3YN5BJUH\\Tang et al. - 2016 - Carbon Storage and Sequestration of Urban Street T.pdf:application/pdf}
