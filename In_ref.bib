@article{harrison_forest_2018,
	title = {Forest bees are replaced in agricultural and urban landscapes by native species with different phenologies and life-history traits},
	volume = {24},
	number = {1},
	journal = {Global Change Biology},
	author = {Harrison, Tina and Gibbs, Jason and Winfree, Rachael},
	year = {2018},
	note = {Publisher: Wiley Online Library},
	pages = {287--296},
	annote = {不同生存策略和生活史特征的蜜蜂对城市的适应性
 
从森林到农田和城市：蜜蜂多样性变化
 
土地覆盖和人类活动改变了物种组成，但到底哪些物种在人工生态系统中留存了下来，是什么特点保证了它们的优势？学者们对传粉者和土地覆盖之间的关系进行了研究，却并未达成共识。本文的作者Harrison等人认为原因是很多研究是在复合的景观中进行取样，难以排除其他土地覆盖类型的影响，而且很多相关性状只适用于解释小尺度上的适应性。针对这些知识缺口和研究不足，Harrison等人在美国北部大范围的森林、农田、城市中对蜜蜂进行采样调查，探究几种生态系统中蜜蜂物种组成的差异，以及相对于森林而言，农田和城市中的蜜蜂在性状上有何特点。本研究于2018年发表在《Global Change Biology》上。
 
数据收集：
样点设置：本研究在美国北部设置了36个样点，样点分布范围跨75,000平方公里。首先设置12个城市样点，彼此间相距25-360公里，然后在各个城市样点5-30公里距离范围内分别设置一个农田和森林样点。为了避免其他土地覆盖类型的干扰，每个样点周围1500米范围内其他土地覆盖类型的比例要低于20\%。此外，森林样点覆盖了调查范围内各类森林。
蜜蜂采集：在每个样点找4个阳光充足的草地，每块草地上设置6个陷阱，即每个样点设置24个陷阱。自2013年开始的3年间共调查11次。调查生长季进行，每次在样点布置陷阱后放置24小时，然后对采集到的蜜蜂进行鉴定。
蜜蜂性状：收集蜜蜂6方面性状数据-寄生性，社会性，食性，筑巢材料，体型大小，和活动高发季节。
分析和结果：
1. 丰度和物种丰富度：以每次收集到的蜜蜂为单元，通过广义线性混合模型 (GLMMs) 分析土地覆盖、季节以及两者交互效应对蜜蜂丰度和物种丰富度的影响。结果表明，虽然平均而言，森林、农田和城市的蜜蜂丰度和物种丰富度无显著差异，但一年之中的变化趋势却不同，森林蜜蜂丰度和物种丰富度在四到五月份达到峰值，而农田和城市蜜蜂的峰值则在六月到八月。
2. 土地覆盖和物种组成及系统发育多样性：通过 Morisita–Horn指数表征样点之间的物种组成差异，并且计算了样点之间物种的系统发育差异，然后通过置换多元方差分析 ( PERMANOVA) 探究了土地覆盖和系统发育组成差异之间的关系。土地覆盖和物种组成及系统发育多样性都相关；森林和其他两者之间的差异大于农田和城市间的差异。
3. 土地覆盖和性状：Fourth-corner test结果表明，有4种性状和土地覆盖相关，即社会性，寄生性，筑巢材料，活动告发季节。比如相比于森林，农田中寄生性的物种较少，筑巢材料为木头的物种也较少；社会性较强的物种在农田和城市中较高。
 
 
 },
	file = {Full Text:C\:\\Users\\kangj\\Documents\\OneDrive\\Zotero\\storage\\UFZNAA24\\Harrison et al. - 2018 - Forest bees are replaced in agricultural and urban.pdf:application/pdf;Snapshot:C\:\\Users\\kangj\\Documents\\OneDrive\\Zotero\\storage\\D9WRPHEL\\gcb.html:text/html;gcb13921-sup-0001-supinfo.docx:C\:\\Users\\kangj\\Documents\\OneDrive\\Zotero\\storage\\H4T6BWPF\\gcb13921-sup-0001-supinfo.docx:application/vnd.openxmlformats-officedocument.wordprocessingml.document}
}

@article{watanabe_latent_2020,
	title = {Latent {Semantic} {Scaling}: {A} {Semisupervised} {Text} {Analysis} {Technique} for {New} {Domains} and {Languages}},
	volume = {0},
	issn = {1931-2458},
	shorttitle = {Latent {Semantic} {Scaling}},
	url = {https://doi.org/10.1080/19312458.2020.1832976},
	doi = {10.1080/19312458.2020.1832976},
	abstract = {Many social scientists recognize that quantitative text analysis is a useful research methodology, but its application is still concentrated in documents written in European languages, especially English, and few sub-fields of political science, such as comparative politics and legislative studies. This seems to be due to the absence of flexible and cost-efficient methods that can be used to analyze documents in different domains and languages. Aiming to solve this problem, this paper proposes a semisupervised document scaling technique, called Latent Semantic Scaling (LSS), which can locate documents on various pre-defined dimensions. LSS achieves this by combining user-provided seed words and latent semantic analysis (word embedding). The article demonstrates its flexibility and efficiency in large-scale sentiment analysis of New York Times articles on the economy and Asahi Shimbun articles on politics. These examples show that LSS can produce results comparable to that of the Lexicoder Sentiment Dictionary (LSD) in both English and Japanese with only small sets of sentiment seed words. A new heuristic method that assists LSS users to choose a near-optimal number of singular values to obtain word vectors that best capture differences between documents on target dimensions is also presented.},
	number = {0},
	urldate = {2021-02-16},
	journal = {Communication Methods and Measures},
	author = {Watanabe, Kohei},
	month = nov,
	year = {2020},
	note = {Publisher: Routledge
\_eprint: https://doi.org/10.1080/19312458.2020.1832976},
	pages = {1--22},
	annote = {日语语义分析
分析模型的要素：
